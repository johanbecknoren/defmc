[project]
title = defmc
version = 0.1
dependencies = https://github.com/andsve/dirtylarry/archive/master.zip

[bootstrap]
main_collection = /graph/graph.collectionc

[input]
game_binding = /input/game.input_bindingc
use_accelerometer = 0

[display]
width = 375
height = 667

[physics]
scale = 0.02

[script]
shared_state = 1

[android]
package = com.defold.android.defmc
manifest = /AndroidManifest.xml

[ios]
bundle_identifier = defold.defmc

[osx]
bundle_identifier = defold.defmc

